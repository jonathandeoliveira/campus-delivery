<h1>Editar transportadora:</h1>

<%= form_with(model: @carrier ) do |f| %>
  <div>
  <%= f.label :company_name %>
  <%= f.text_field :company_name %>
  </div>

  <div>
  <%= f.label :company_register %>
  <%= f.text_field :company_register %>
  </div>

  <div>
  <%= f.label :brand_name %>
  <%= f.text_field :brand_name %>
  </div>

  <div>
  <%= f.label :city %>
  <%= f.text_field :city %>
  </div>

  <div>
  <%= f.label :state %>
  <%= f.text_field :state %>
  </div>

  <div>
  <%= f.label :adress %>
  <%= f.text_field :adress %>
  </div>

  <div>
  <%= f.label :email_domain %>
  <%= f.text_field :email_domain %>
  </div>

  <div>
  <%= f.submit 'Salvar' %>
  </div>
<% end %>